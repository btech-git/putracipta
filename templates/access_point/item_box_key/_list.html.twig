{% import 'app/data_grid.html.twig' as grid %}

{{ grid.form(form) }}

{{ grid.info(form, count, itemBoxKeys) }}

<table class="table table-bordered table-striped">
    <thead>
        <tr class="table-primary" {{ grid.sort_link(form) }}>
            <th role="button" {{ grid.sort_link_item(form, 'abc') }}>
                Abc {{ grid.sort_char(form.sort.abc.vars.value) }}
            </th>
            <th role="button" {{ grid.sort_link_item(form, 'def') }}>
                Def {{ grid.sort_char(form.sort.def.vars.value) }}
            </th>
            <th role="button" {{ grid.sort_link_item(form, 'ghi') }}>
                Ghi {{ grid.sort_char(form.sort.ghi.vars.value) }}
            </th>
            <th role="button" {{ grid.sort_link_item(form, 'jkl') }}>
                Jkl {{ grid.sort_char(form.sort.jkl.vars.value) }}
            </th>
            <th role="button" {{ grid.sort_link_item(form, 'mno') }}>
                Mno {{ grid.sort_char(form.sort.mno.vars.value) }}
            </th>
            <th role="button" {{ grid.sort_link_item(form, 'pqrs') }}>
                Pqrs {{ grid.sort_char(form.sort.pqrs.vars.value) }}
            </th>
            <th role="button" {{ grid.sort_link_item(form, 'tuv') }}>
                Tuv {{ grid.sort_char(form.sort.tuv.vars.value) }}
            </th>
            <th role="button" {{ grid.sort_link_item(form, 'wxyz') }}>
                Wxyz {{ grid.sort_char(form.sort.wxyz.vars.value) }}
            </th>
            <th role="button" {{ grid.sort_link_item(form, 'qwerty') }}>
                Qwerty {{ grid.sort_char(form.sort.qwerty.vars.value) }}
            </th>
            <th role="button" {{ grid.sort_link_item(form, 'dvorak') }}>
                Dvorak {{ grid.sort_char(form.sort.dvorak.vars.value) }}
            </th>
            <th></th>
        </tr>
        <tr class="table-light" {{ grid.filter_link(form) }}>
            <th>
                <input type="text" {{ grid.filter_link_item(form, 'abc') }} value="{{ form.filter.abc.1.vars.value }}" />
            </th>
            <th>
                <input type="text" {{ grid.filter_link_item(form, 'def') }} value="{{ form.filter.def.1.vars.value }}" />
            </th>
            <th>
                <input type="text" {{ grid.filter_link_item(form, 'ghi') }} value="{{ form.filter.ghi.1.vars.value }}" />
            </th>
            <th>
                <input type="text" {{ grid.filter_link_item(form, 'jkl') }} value="{{ form.filter.jkl.1.vars.value }}" />
            </th>
            <th>
                <input type="text" {{ grid.filter_link_item(form, 'mno') }} value="{{ form.filter.mno.1.vars.value }}" />
            </th>
            <th>
                <input type="text" {{ grid.filter_link_item(form, 'pqrs') }} value="{{ form.filter.pqrs.1.vars.value }}" />
            </th>
            <th>
                <input type="text" {{ grid.filter_link_item(form, 'tuv') }} value="{{ form.filter.tuv.1.vars.value }}" />
            </th>
            <th>
                <input type="text" {{ grid.filter_link_item(form, 'wxyz') }} value="{{ form.filter.wxyz.1.vars.value }}" />
            </th>
            <th>
                <input type="text" {{ grid.filter_link_item(form, 'qwerty') }} value="{{ form.filter.qwerty.1.vars.value }}" />
            </th>
            <th>
                <input type="text" {{ grid.filter_link_item(form, 'dvorak') }} value="{{ form.filter.dvorak.1.vars.value }}" />
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        {% for itemBoxKey in itemBoxKeys %}
            <tr>
                <td>{{ itemBoxKey.abc }}</td>
                <td>{{ itemBoxKey.def }}</td>
                <td>{{ itemBoxKey.ghi }}</td>
                <td>{{ itemBoxKey.jkl ? itemBoxKey.jkl|date('Y-m-d') : '' }}</td>
                <td>{{ itemBoxKey.mno ? itemBoxKey.mno|date('H:i:s') : '' }}</td>
                <td>{{ itemBoxKey.pqrs ? itemBoxKey.pqrs|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ itemBoxKey.tuv ? 'Yes' : 'No' }}</td>
                <td>{{ itemBoxKey.wxyz }}</td>
                <td>{{ itemBoxKey.qwerty ? itemBoxKey.qwerty|join(', ') : '' }}</td>
                <td>{{ itemBoxKey.dvorak }}</td>
                <td>
                    <a class="btn btn-info btn-sm" href="{{ path('app_access_point_item_box_key_show', {'id': itemBoxKey.id}) }}">Show</a>
                    <a class="btn btn-warning btn-sm" href="{{ path('app_access_point_item_box_key_edit', {'id': itemBoxKey.id}) }}">Edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="11">No records found</td>
            </tr>
        {% endfor %}
    </tbody>
</table>

{{ grid.navigation(form, count, itemBoxKeys) }}
