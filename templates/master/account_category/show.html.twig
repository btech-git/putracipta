{% extends 'base.html.twig' %}

{% block title %}AccountCategory{% endblock %}

{% block body %}
    <h1>AccountCategory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ account_category.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ account_category.code }}</td>
            </tr>
            <tr>
                <th>Name</th>
