{{ form_start(form) }}
    {{ form_row(form.customer, {'attr': {'readonly': dielineMillar.id is not null}}) }}
    
    {% if (dielineMillar.id is not null) %}
        <hr />

        <legend class="float-none w-auto text-dark px-1">Millar List</legend>
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Code</th>
                    <th>Name</th>
                    <th>Jml</th>
                    <th>Jml Up Cetak</th>
                    <th>Layout Cetak</th>
                </tr>
            </thead>
            <tbody>
                {% for lastDielineMillar in lastDielineMillars %}
                    <tr>
                        <td>{{ lastDielineMillar.codeNumber }}</td>
                        <td>{{ lastDielineMillar.name }}</td>
                        <td>{{ lastDielineMillar.quantity }}</td>
                        <td>{{ lastDielineMillar.quantityUpPrinting }}</td>
                        <td>{{ lastDielineMillar.printingLayout }}</td>
                    </tr>
                {% endfor %}
            </tbody>
        </table>

        <hr />
    {% endif %}
    
    {{ form_rest(form) }}
    <div class="d-grid">
        <button class="btn btn-secondary"
                data-controller="dom-element"
                data-action="dom-element#confirm"
                data-dom-element-confirm-message-param="Are you sure you want to save this item?">
            Save
        </button>
    </div>
{{ form_end(form) }}
