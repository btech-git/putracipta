{
    "subTotalAfterTaxInclusion": "{{ saleOrderHeader.subTotalAfterTaxInclusion|default|number_format(2) }}",
    "discountNominal": "{{ saleOrderHeader.discountNominal|default|number_format(2) }}",
    "taxNominal": "{{ saleOrderHeader.taxNominal|default|number_format(2) }}",
    "shippingFee": "{{ saleOrderHeader.shippingFee|default|number_format(2) }}",
    "grandTotal": "{{ saleOrderHeader.grandTotal|default|number_format(2) }}",
    "saleOrderDetails": {
        {%- for saleOrderDetailForm in form.saleOrderDetails -%}
            {%- set saleOrderDetail = saleOrderDetailForm.vars.data -%}
            "{{ saleOrderDetailForm.vars.name }}": {
                "total": "{{ saleOrderDetail.total|default|number_format(2) }}"
            }
            {%- if not loop.last -%},{%- endif -%}
        {%- endfor -%}
    }
}
